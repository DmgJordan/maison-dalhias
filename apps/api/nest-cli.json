{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      { "include": "pdf/assets/**/*", "outDir": "dist/src" },
      { "include": "pdf/fonts/**/*", "outDir": "dist/src" }
    ]
  }
}
